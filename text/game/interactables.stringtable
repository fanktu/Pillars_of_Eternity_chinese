			<ID>313</ID>
			<DefaultText>雌狐地洞</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>314</ID>
			<DefaultText>塵世奇景套房</DefaultText>
			<FemaleText/>
		</Entry>
