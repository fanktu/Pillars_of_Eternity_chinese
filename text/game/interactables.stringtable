			<DefaultText>艾德邁斯巢穴</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>330</ID>
			<DefaultText>巧匠廳加成</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
